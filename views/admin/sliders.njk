{% include "admin-includes/sliders/_slider-fixed-dragg.njk" %}
</div>
<div class="text-center m-b">
  <h3 class="m-b-0" {{dc}}>{{header2}}</h3>
  <small>{{sub2}}</small>
</div>
{% include "admin-includes/sliders/_slider-currency.njk" %}
<div class="row gutter-xs">
{% include "admin-includes/sliders/_slider-vertical.njk" %}
{% include "admin-includes/sliders/_slider-vertical-img.njk" %}
</div>
