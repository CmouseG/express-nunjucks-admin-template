<div class="col-sm-6 col-md-4" data-toggle="match-height">
  <h5 class="text-center">Custom content</h5>
  <p class="text-center text-muted">
    <small>Linkify list group items by using anchor tags</small>
  </p>
  <div class="list-group">
    <a class="list-group-item" href="{{JSV}}">
      <div class="pull-left m-r">
        <img class="circle" width="40" height="40" src="{{avatar}}">
      </div>
      <div class="clear">
        <small class="pull-right">{{Date}}</small>
        <h5 class="list-group-item-heading">{{Name}}</h5>
        <small class="truncate lor-s7"></small>
      </div>
    </a>
    <a class="list-group-item" href="{{JSV}}">
      <div class="pull-left m-r">
        <img class="circle" width="40" height="40" src="{{avatar}}">
      </div>
      <div class="clear">
        <small class="pull-right">{{Date}}</small>
        <h5 class="list-group-item-heading">{{Name}}</h5>
        <small class="truncate lor-s5"></small>
      </div>
    </a>
    <a class="list-group-item active" href="{{JSV}}">
      <div class="pull-left m-r">
        <img class="circle" width="40" height="40" src="{{avatar}}">
      </div>
      <div class="clear">
        <small class="pull-right">{{Date}}</small>
        <h5 class="list-group-item-heading">{{Name}}</h5>
        <small class="truncate lor-s1"></small>
      </div>
    </a>
  </div>
</div>
